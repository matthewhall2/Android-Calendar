<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:orientation="vertical"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent" android:id="@+id/linearLayout2">
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Enter new event name"
            android:ems="10"
            android:id="@+id/enternewname" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="81dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="78dp" android:layout_marginBottom="52dp"
            app:layout_constraintBottom_toTopOf="@id/enternewstart" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="120dp" app:layout_constraintHorizontal_bias="1.0"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Enter new start date"
            android:ems="10"
            android:id="@+id/enternewstart"
            app:layout_constraintTop_toBottomOf="@id/enternewname" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="78dp" android:layout_marginBottom="500dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="120dp" app:layout_constraintHorizontal_bias="0.0" android:layout_marginTop="8dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Enter new end date"
            android:ems="10"
            android:id="@+id/enternewend" android:layout_marginTop="70dp"
            app:layout_constraintTop_toBottomOf="@id/enternewstart" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="76dp" android:layout_marginBottom="300dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.729"/>
    <Button
            android:text="Back to edit event menu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button7"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="79dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="126dp" android:layout_marginBottom="83dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="129dp" app:layout_constraintTop_toBottomOf="@+id/enternewend"
            app:layout_constraintVertical_bias="1.0"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Enter new start time"
            android:ems="10"
            android:id="@+id/ednewstarttime" android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/enternewstart" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="82dp" android:layout_marginBottom="35dp"
            app:layout_constraintBottom_toTopOf="@+id/enternewend" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="116dp" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.976"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Enter new end time"
            android:ems="10"
            android:id="@+id/ednewendtime" android:layout_marginTop="38dp"
            app:layout_constraintTop_toBottomOf="@+id/enternewend" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="82dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="116dp"
    />
    <TextView
            android:text="  "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/wrong_start"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/ednewstarttime" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="133dp" android:layout_marginBottom="9dp"
            app:layout_constraintBottom_toTopOf="@+id/enternewend" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="219dp"/>
    <Button
            android:text="set name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/changeeventname"
            android:layout_marginStart="16dp"
            app:layout_constraintStart_toEndOf="@+id/enternewname" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="78dp"/>
    <Button
            android:text="set start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/changeeventstart"
            android:layout_marginTop="105dp"
            app:layout_constraintTop_toBottomOf="@+id/changeeventname" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="303dp"/>
    <Button
            android:text="set end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/changeeventend"
            android:layout_marginTop="127dp"
            app:layout_constraintTop_toBottomOf="@+id/changeeventstart" android:layout_marginStart="9dp"
            app:layout_constraintStart_toEndOf="@+id/ednewendtime"/>
</androidx.constraintlayout.widget.ConstraintLayout>